{
  "extends": "airbnb/base",
  "parser": "babel-eslint",
  "rules": {
    "max-len": [2, 120],
    "comma-dangle": 0,
    "no-console": 0,
    "semi": [2, "never"],
    "space-before-function-paren": 0,
    "no-multi-spaces": 0,
    "no-param-reassign": 0,
    "no-return-assign": 0,
    "no-use-before-define": [2, "nofunc"],
    "no-confusing-arrow": 0,
    "camelcase": 0
  }
}
